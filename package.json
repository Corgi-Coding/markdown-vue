{
  "name": "@corgicoding/markdown-editor",
  "version": "0.0.1",
  "private": false,
  "description": "Vue components based on monaco",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Corgi-Coding/markdown-vue.git"
  },
  "bugs": {
    "url": "https://github.com/Corgi-Coding/markdown-vue/issues"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "keywords": [
    "editor",
    "vue",
    "monaco",
    "corgicoding",
    "Corgi Coding",
    "Corgi-Coding",
    "corgi coding",
    "corgi-coding"
  ],
  "author": "Charles Chan",
  "license": "Apache 2.0",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./style.css": "./lib/style.css",
    "./lib/style.css": "./lib/style.css",
    "./types.d": "./lib/types.d.ts"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "monaco-editor": "^0.48.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sass": "^1.77.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^2.0.6"
  }
}
